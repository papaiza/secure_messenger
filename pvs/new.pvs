message [U,G : TYPE] : THEORY
BEGIN

	TEXT: TYPE+
UID : TYPE+
	MID : TYPE+
	GID : TYPE+
  MSG_INFO: TYPE = 
    [# 
      content: TEXT
    #]

    text: TEXT

STATE: TYPE =
  [# 
    users: set[UID]
   ,groups: set[GID]
   ,msgs: set[MID]
   ,membership: set[[(users),(groups)]]
   ,info : [(msgs) -> MSG_INFO[text]]   
     #]
END message


